
<!DOCTYPE html>
<html>
<head>
<title>List Page</title>
</head>
<body>
   <h1>List Page</h1>
   <!-- <a href='/insert'>Insert Data</a> -->
   <hr/>
   <table width="100%" border="1">
      <tr>
         <th>Num</th>
         <th>Task</th>
         <th>Due Date</th>
         <th>Priority</th>
         <th>Status</th>
      </tr>
  <% prodList.forEach(function(item, index){ %>
      <tr>
         <!-- <td><a href='/delete/<%= item.id %>' >DELETE</a></td>
         <td><a href='/edit/<%= item.id %>' >EDIT</a></td> -->
         <td><%= item.id %></td>
         <td><%= item.task %></td>
         <td><%= item.due_date %></td>
         <td><%= item.priority %></td>
         <td><%= item.status %></td>
     </tr>
  <% }); %>
 </table>
</body>
</html>